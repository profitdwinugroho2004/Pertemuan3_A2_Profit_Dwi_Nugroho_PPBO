class Person:
    def __init__(self, name, age):
        self.name = name
        self.age = age

# Membuat objek        
person1 = Person("Andi", 20)

# Menampilkan atribut objek sebelum dihapus
print("Sebelum dihapus : ")
print("Nama : ", person1.name)
print("Umur : ", person1.age)

# Menghapus atribut objek
del person1.age

# Menampilkan atribut objek setelah dihapus
print("\nSetelah dihapus atribut 'age' :")
print("Nama :", person1.name)

# Mencoba mengakses atribut yang telah dihapus dan akan menghasilkan AttributeError
# print("Umur :", person1.age)

# Menghapus objek itu sendiri
del person1
